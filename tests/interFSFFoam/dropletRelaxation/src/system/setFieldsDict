FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.liquid 0
    volScalarFieldValue p_rgh 1e5
    volScalarFieldValue p 1e5
    volScalarFieldValue T 300
);

regions
(
    boxToCell
    {
        box (-1e6 -1e6 -1e6) (1e6 1e6 0.25); 
        fieldValues
        (
            volScalarFieldValue alpha.liquid 1
            volScalarFieldValue p_rgh 1e5
            volScalarFieldValue p 1e5
            volScalarFieldValue T 300
        );
    }

