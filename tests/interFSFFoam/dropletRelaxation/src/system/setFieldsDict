FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.liquid 1
);

regions
(
    boxToCell
    {
        box (-0.5 -0.5 -0.5) (0.5 0.5 0.5); 
        fieldValues
        (
            volScalarFieldValue alpha.liquid 0
        );
    }
);


