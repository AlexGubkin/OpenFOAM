FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
phases (liquid gas);

liquid
{
    transportModel  Newtonian;
    nu              1e-06;
    rho             1000;
}

gas
{
    transportModel  Newtonian;
    nu              1e-5;
    rho             1.2527;
}

sigma           0.046;

transportModel  Newtonian;


