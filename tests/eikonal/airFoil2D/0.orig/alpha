FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    //- Set patchGroups for constraint patches
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type    zeroGradient;
    }

    outlet
    {
        type    zeroGradient;
    }

    walls
    {
        type    zeroGradient;
    }

    frontAndBack
    {
        type    empty;
    }
}


